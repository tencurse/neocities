  <title>{{ meta.site_title }} {{ '| '+title if title != meta.site_title }}</title>

  <link rel="stylesheet" href="/assets/css/theme.css" />
  <link rel="stylesheet" href="/assets/css/prism.css" />
  <link rel="stylesheet" href="/assets/css/styles.css" />

  {% if page.url == '/' %}
  <link rel="stylesheet" href="https://neossg.neocities.org/onionring.css" />
  <link rel="stylesheet" href="https://teethinvitro.neocities.org/webring/linuxring/script/onionring.css" />
  {% endif %}

  <link rel="me" href="https://tech.lgbt/@karma" />
  <link rel="pgpkey" href="application/pgp-keys" href="/assets/etc/pgp.asc">

  <link rel="shortcut icon" href="/assets/img/b08-icon-pc.gif" type="image/gif">

  <link rel="preload" href="/assets/fonts/MS-PGothic.woff2" as="font" type="font/woff2" crossorigin="">
